{
  "mcpServers": {
    "extend-mcp": {
      "command": "docker",
      "args": [
        "run", "--rm", "-i",
        "-p", "3000:3000",
        "-e", "PORT=3000",
        "-e", "LOG_LEVEL=info",
        "-e", "CONFIG_DIR=/app/config",
        "-v", "/path/to/config:/app/config:ro",
        "extend-sdk-mcp-server:latest"
      ]
    }
  }
}


